<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="lassaTireId" class="_02.spring.model.Lassa"/>
	
	<bean id="goodYearId" class="_02.spring.model.GoodYear"/>
	
	<bean id="petlasid" class="_02.spring.model.Petlas"/>

	<bean id="carId" class="_02.spring.model.Car">
		<property name="wheel" ref="lassaTireId"/>
	</bean>


</beans>